{
  "name": "asi-ngtools",
  "version": "6.0.0-rc16",
  "main": "./dist/index.js",
  "scripts": {
    "build": "gulp build",
    "babel": "node node_modules/babel-cli/bin/babel.js dist --out-dir dist --presets es2015",
    "package": "npm run build && npm run babel",
    "build:watch": "gulp",
    "docs": "npm run docs:build",
    "docs:build": "compodoc -p tsconfig.json -n asi-ngtools -d docs --hideGenerator",
    "docs:serve": "npm run docs:build -- -s",
    "docs:watch": "npm run docs:build -- -s -w",
    "lint": "tslint --type-check --project tsconfig.json src/**/*.ts",
    "lite": "lite-server",
    "test": "tsc && karma start",
    "deploy-to-git": "deploy-to-git"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xrobert35/asi-ngtools"
  },
  "author": {
    "name": "asi-ngtools",
    "email": "xrobert@asi.fr"
  },
  "keywords": [
    "angular"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xrobert35/asi-ngtools/issues"
  },
  "devDependencies": {
    "@angular/animations": "^6.0.0",
    "@angular/common": "^6.0.0",
    "@angular/compiler": "^6.0.0",
    "@angular/compiler-cli": "^6.0.0",
    "@angular/core": "^6.0.0",
    "@angular/forms": "^6.0.0",
    "@angular/http": "^6.0.0",
    "@angular/platform-browser": "^6.0.0",
    "@angular/platform-browser-dynamic": "^6.0.0",
    "@angular/router": "^6.0.0",
    "@compodoc/compodoc": "^1.0.0-beta.10",
    "@ngx-translate/core": "^9.1.1",
    "@types/jasmine": "2.5.53",
    "@types/lodash": "^4.14.66",
    "@types/node": "~6.0.60",
    "angular-in-memory-web-api": "^0.3.2",
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "codelyzer": "~3.2.0",
    "concurrently": "^3.4.0",
    "core-js": "^2.4.1",
    "del": "^2.2.2",
    "deploy-to-git": "0.1.5",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^3.6.0",
    "gulp-concat": "^2.5.2",
    "gulp-less": "3.1.0",
    "gulp-load-plugins": "^0.10.0",
    "gulp-rename": "^1.2.2",
    "gulp-rollup": "^2.15.0",
    "gulp-sizereport": "^1.1.3",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lite-server": "^2.3.0",
    "lodash": "^4.17.10",
    "moment": "^2.18.1",
    "node-sass": "^4.5.2",
    "node-sass-tilde-importer": "^1.0.0",
    "node-watch": "^0.5.2",
    "protractor": "~5.1.2",
    "rollup": "^0.54.1",
    "run-sequence": "^1.2.2",
    "rxjs": "^6.2.0",
    "systemjs": "^0.20.12",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": "~2.7.2",
    "zone.js": "^0.8.26"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "config": {
    "deployToGit": {
      "repository": "git@cdsgit.asi-rennes.fr:asi-expertise/asi-ngtools.git",
      "branch": "$npm_package_version",
      "folder": "dist",
      "script": "npm run build",
      "commit": "Mise en place de la version : $npm_package_version",
      "user": {
        "email": "xrobert@asi.fr",
        "name": "xrobert"
      }
    }
  }
}
